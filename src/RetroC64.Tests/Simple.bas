10 REM --- C64 BASIC TEST PROGRAM ---
20 PRINT "STARTING TEST PROGRAM"
30 DIM A(5),S$(5)
40 FOR I=0 TO 5:A(I)=I^2:NEXT
50 FOR I=0 TO 5:S$(I)="VAL="+STR$(A(I)):NEXT
60 FOR I=0 TO 5:PRINT "A(";I;")=";A(I);", S$=";S$(I):NEXT
70 LET X=10:Y=20:Z=X+Y
80 IF Z=30 THEN PRINT "Z=30 OK":GOTO 100
90 PRINT "Z CHECK FAILED":END
100 GOSUB 300
110 FOR I=1 TO 3:READ N:PRINT "READ N=";N:NEXT
120 DATA 1,2,3
130 A$="HELLO":B$="WORLD"
140 C$=A$+" "+B$:PRINT C$
150 PRINT LEFT$(C$,5),MID$(C$,7,5),RIGHT$(C$,5)
160 PRINT LEN(C$),ASC("A"),CHR$(65)
170 PRINT "RND TEST:",RND(1),INT(RND(1)*10)
180 X=PEEK(53280):PRINT "BORDER COLOR=";X
190 POKE 53280,6:REM SET BORDER TO BLUE
200 POKE 54296,15:POKE 54277,33:POKE 54278,128
210 POKE 54273,0:POKE 54272,100:REM SOUND
220 DEF FNX(Y)=Y*Y:PRINT "FNX(5)=";FNX(5)
230 OPEN 1,8,2,"0:TESTFILE,S,W":PRINT#1,"HELLO":CLOSE 1
240 OPEN 2,8,2,"0:TESTFILE,S,R":INPUT#2,A$:CLOSE 2:PRINT A$
250 IF A$="HELLO" THEN PRINT "FILE I/O OK"
260 REM MULTI-STATEMENT LINE
270 I=5:J=6:K=I+J:PRINT"K=";K
280 FOR I=1 TO 2:FOR J=1 TO 2:PRINT I;J:NEXT J:NEXT I
290 PRINT "END OF MAIN":END
300 PRINT "IN SUBROUTINE":RETURN